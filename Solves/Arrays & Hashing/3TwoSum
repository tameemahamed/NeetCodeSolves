#define pb push_back 
class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        vector<int> ans;
        for(int i=0;i<nums.size();i++){
            for(int j=0;j<nums.size();j++){
                if(i==j) continue;
                if(nums[i]+nums[j]==target){
                    ans.pb(i);
                    ans.pb(j);
                    break;
                }
            }
            if(ans.size()==2) break;
        }
        return ans;
    }
};
